/*
	Arbre binaire
*/
digraph G {
	bgcolor="#FFFFFF";
	"N()" [label="A"];
	"N(0)" [label="B"];
	"N(00)" [color="#FFFFFF", label=""];
	"N(0)" -> "N(00)";
	"N(01)" [label="C"];
	"N(010)" [color="#FFFFFF", label=""];
	"N(01)" -> "N(010)";
	"N(011)" [color="#FFFFFF", label=""];
	"N(01)" -> "N(011)";
	"N(0)" -> "N(01)";
	"N()" -> "N(0)";
	"N(1)" [label="D"];
	"N(10)" [color="#FFFFFF", label=""];
	"N(1)" -> "N(10)";
	"N(11)" [color="#FFFFFF", label=""];
	"N(1)" -> "N(11)";
	"N()" -> "N(1)";

}
